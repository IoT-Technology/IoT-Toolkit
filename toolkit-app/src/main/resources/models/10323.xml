<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
  <Object ObjectType="MODefinition">
    <Name>CCU Control</Name>
    <Description1><![CDATA[This LWM2M Object includes all operations on CCUs.]]></Description1>
    <ObjectID>10323</ObjectID>
    <ObjectURN>urn:oma:lwm2m:x:10323</ObjectURN>
    <LWM2MVersion>1.0</LWM2MVersion>
    <ObjectVersion>1.0</ObjectVersion>
    <MultipleInstances>Multiple</MultipleInstances>
    <Mandatory>Optional</Mandatory>
    <Resources>
		<!--Item ID="0">
			Object ID of CCU Control should be the same with that of CCU, so no need for CCU resource 
			<Name>CCU</Name>
			<Operations>RW</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type>Objlnk</Type>
			<RangeEnumeration/>
			<Units/>
			<Description>
			  <![CDATA[Contains the reference to the CCU which should be controlled.]]>
			</Description>
		</Item-->
		<Item ID="100">
			<Name>CCU Restart</Name>
			<Operations>E</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type/>
			<RangeEnumeration/>
			<Units/>
			<Description><![CDATA[Restart the CCU.]]></Description>
		</Item>
		<Item ID="101">
			<Name>CCU Reset</Name>
			<Operations>E</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type/>
			<RangeEnumeration/>
			<Units/>
			<Description><![CDATA[Reset the CCU to factory settings.]]></Description>
		</Item>	
		<Item ID="102">
			<Name>CCU Self-checking</Name>
			<Operations>E</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type/>
			<RangeEnumeration/>
			<Units/>
			<Description><![CDATA[Let the CCU to do Self-checking.]]></Description>
		</Item>	
    </Resources>
    <Description2 />
  </Object>
</LWM2M>
