<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
  <Object ObjectType="MODefinition">
    <Name>SCA PM</Name>
    <Description1><![CDATA[This LWM2M Object includes all PM items belongs to a specific SCA.]]></Description1>
    <ObjectID>10328</ObjectID>
    <ObjectURN>urn:oma:lwm2m:x:10328</ObjectURN>
    <LWM2MVersion>1.0</LWM2MVersion>
    <ObjectVersion>1.0</ObjectVersion>
    <MultipleInstances>Multiple</MultipleInstances>
    <Mandatory>Optional</Mandatory>
    <Resources>
		<Item ID="1">
			<Name>SCA Name</Name>
			<Operations>R</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type>String</Type>
			<RangeEnumeration/>
			<Units/>
			<Description>
			  <![CDATA[The name of the SCA.]]>
			</Description>
		</Item>

	    <Item ID="605">
			<Name>SCA Current</Name>
			<Operations>R</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type>Float</Type>
			<RangeEnumeration/>
			<Units>A</Units>
			<Description><![CDATA[The current of the SCA.]]></Description>
        </Item>
	    <Item ID="606">
			<Name>SCA Temperate</Name>
			<Operations>R</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type>Float</Type>
			<RangeEnumeration/>
			<Units>Cel</Units>
			<Description><![CDATA[SCA Temperature.]]></Description>
        </Item>
    </Resources>
    <Description2 />
  </Object>
</LWM2M>
