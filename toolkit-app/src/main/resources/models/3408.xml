<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.openmobilealliance.org/tech/profiles/LWM2M-v1_1.xsd">
	<Object ObjectType="MODefinition">
		<Name>Manhole Cover</Name>
		<Description1><![CDATA[Describe the status of a manhole cover.]]></Description1>
		<ObjectID>3408</ObjectID>
		<ObjectURN>urn:oma:lwm2m:ext:3408</ObjectURN>
		<LWM2MVersion>1.1</LWM2MVersion>
		<ObjectVersion>1.0</ObjectVersion>
		<MultipleInstances>Multiple</MultipleInstances>
		<Mandatory>Optional</Mandatory>
		<Resources>
			<Item ID="1">
				<Name>Safety Angle Threshold</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Integer</Type>
				<RangeEnumeration>0..180</RangeEnumeration>
				<Units>rad</Units>
				<Description><![CDATA[The threshold of manhole cover's safety angle.]]></Description>
			</Item>
			<Item ID="2">
				<Name>Manhole Cover Tilt</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Integer</Type>
				<RangeEnumeration>0..180</RangeEnumeration>
				<Units>rad</Units>
				<Description><![CDATA[The tilt angle of a manhole cover.]]></Description>
			</Item>
			<Item ID="3">
				<Name>Cover Open Status</Name>
				<Operations>R</Operations>
				<MultipleInstances>Single</MultipleInstances>
				<Mandatory>Mandatory</Mandatory>
				<Type>Boolean</Type>
				<RangeEnumeration></RangeEnumeration>
				<Units></Units>
				<Description><![CDATA[Indicate whether an open status of the manhole cover is detected. Not detected open [true] or Detected open [false].]]></Description>
			</Item>
			
			
		</Resources>
		<Description2 />
	</Object>
</LWM2M>
