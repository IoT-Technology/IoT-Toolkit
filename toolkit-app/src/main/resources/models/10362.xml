<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
  <Object ObjectType="MODefinition">
    <Name>Drop Sensor</Name>
    <Description1><![CDATA[This LWM2M Object includes Drop Sensor information.]]></Description1>
    <ObjectID>10362</ObjectID>
    <ObjectURN>urn:oma:lwm2m:x:10362</ObjectURN>
    <LWM2MVersion>1.0</LWM2MVersion>
    <ObjectVersion>1.0</ObjectVersion>
    <MultipleInstances>Multiple</MultipleInstances>
    <Mandatory>Optional</Mandatory>
    <Resources>
		<Item ID="1">
			<Name>Name</Name>
			<Operations>R</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type>String</Type>
			<RangeEnumeration/>
			<Units/>
			<Description>
				<![CDATA[The display name of the Drop Sensor.]]>
			</Description>
		</Item>
		<Item ID="50">
			<Name>Status</Name>
			<Operations>R</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type>Integer</Type>
			<RangeEnumeration/>
			<Units/>
			<Description><![CDATA[0:Normal, 1:Unknown, <0:Failed with error code(negative number).]]></Description>
        </Item>
		<Item ID="51">
			<Name>Drop Detection</Name>
			<Operations>R</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Optional</Mandatory>
			<Type>Boolean</Type>
			<RangeEnumeration/>
			<Units/>
			<Description><![CDATA[If Drop Detection is enabled or not,
				the setting of which is a Boolean value (1,0) where 1 is Enabled and 0 is Disabled.]]></Description>
		</Item>		
		
	  	<Item ID="100">
			<Name>Drop Detection Control</Name>
			<Operations>E</Operations>
			<MultipleInstances>Single</MultipleInstances>
			<Mandatory>Mandatory</Mandatory>
			<Type/>
			<RangeEnumeration/>
			<Units/>
			<Description><![CDATA[Enable/Disable Drop Detection: Parameter: 1:Enable, 0:Disable]]></Description>
		</Item>
    </Resources>
    <Description2 />
  </Object>
</LWM2M>
